{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
    "containerSasToken": { "type": "securestring" },
    "softcatDynamicsCode": {
      "type": "string",
      "maxLength": 6,
      "metadata": {
        "description": "Softcat customer dynamics code. Must be a valid 6 character code."
      }
    }
    },
    "variables":{
    "templateBaseUrl": "https://sctdeploymenttemplates.blob.core.windows.net/live/",
    "patchingTemplateUrl": "[concat(uri(variables('templateBaseUrl'), 'Azure-Patching-Template.json', parameters('containerSasToken')))]"
    },
    "resources": [
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2019-10-01",
      "name": "AzurePatchingTemplate",
      "properties": {
      "mode": "Incremental",
      "templateLink": {
        "uri": "[variables('patchingTemplateUrl')]",
        "contentVersion": "1.0.0.0"
      },
      "parameters": {
        "softcatDynamicsCode" :{"value": "[parameters('softcatDynamicsCode')]"}
      }
      }
    }
    ],
    "outputs": {
    }
  }